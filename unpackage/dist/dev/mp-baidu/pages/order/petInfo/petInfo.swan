<view class="page"><block s-if="{{isok}}"><view class="container"><image class="bannerImg" src="../../../static/shop/petbanner.png" mode="aspectFill"></image><view class="percentBall"><view class="ball">30%</view><text class="t1">完善订单信息</text><image class="toTop" src="../../../static/shop/toTop.png" mode="aspectFit"></image></view><view class="petBox1"><view data-event-opts="{{[['tap',[['petType',['$event']]]]]}}" bindtap="__e"><text class="name1">宠物类型</text><image class="toRight" src="../../../static/mine/rightGray.png" mode="aspectFit"></image></view><picker value="{{index}}" range="{{ageArr}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="picker"><text class="name1">宠物年龄</text><image class="toRight" src="../../../static/mine/rightGray.png" mode="aspectFit"></image></view></picker></view><view class="petBox1"><picker mode="date" value="{{date}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker"><text class="name1">逝去时间</text><image class="toRight" src="../../../static/mine/rightGray.png" mode="aspectFit"></image></view></picker><view data-event-opts="{{[['tap',[['getAddress',['$event']]]]]}}" class="picker" bindtap="__e"><text class="name1">逝去地点</text><image class="toRight" src="../../../static/mine/rightGray.png" mode="aspectFit"></image></view><picker value="{{index}}" range="{{resonArr}}" data-event-opts="{{[['change',[['bingResonChange',['$event']]]]]}}" bindchange="__e"><view class="picker"><text class="name1">逝去原因</text><image class="toRight" src="../../../static/mine/rightGray.png" mode="aspectFit"></image></view></picker></view><view class="mBox"><text class="m1">备注</text><textarea class="tx1" maxlength="-1" placeholder="可填写跟车人数、搬运宠物用品等需要" data-event-opts="{{[['input',[['__set_model',['','reMark','$event',[]]]]]]}}" value="{{reMark}}" bindinput="__e"></textarea></view><view data-event-opts="{{[['tap',[['keep',['$event']]]]]}}" class="keepBtn" bindtap="__e">保存</view></view></block></view>