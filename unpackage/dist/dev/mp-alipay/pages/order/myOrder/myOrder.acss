  padding-top: 20rpx;
  width: 686rpx;
  line-height: 40rpx;
  border-radius: 20rpx;
  background-color: white;
  border: 2rpx solid rgba(255, 255, 255, 0);
  padding-bottom: 24rpx;
}
.orderCard .numName {
  margin-left: 24rpx;
  color: #101010;
  font-size: 24rpx;
