<view class="page"><block a:if="{{isok}}"><view class="conatiner"><view class="backCard"><image class="headImg" src="{{headImg}}"></image><view class="nickName">{{nickName}}</view></view><block a:for="{{imgList}}" a:for-item="item" a:for-index="index" a:key="index"><view><button class="myList" open-type="{{index==2?'contact':''}}" data-event-opts="{{[['tap',[['toPage',[index]]]]]}}" onTap="__e"><image class="img" src="{{item}}"></image><text class="myListName">{{titList[index]}}</text><image class="toPage" src="../../../static/mine/rightGray.png"></image></button></view></block></view></block><block a:else><view class="container-loading"><image src="../../static/loading.gif"></image></view></block></view>