<view class="page"><view class="container"><view class="navbar"><view class="navbar-search"><view class="navbar-search-input"><input placeholder="搜索宠物/地区/关键词等" onConfirm="searchconfirm" data-event-opts="{{[['input',[['inputTyping',['$event']]]]]}}" value="{{petVarietyName}}" onInput="__e"/><block a:if="{{petVarietyName.length>0}}"><view data-event-opts="{{[['tap',[['clearInput',['$event']]]]]}}" class="weui-icon-clear" onTap="__e"><icon type="cancel" size="14"></icon></view></block></view></view></view><block a:if="{{petVarietyName.length>0}}"><view class="bgwh"><block a:for="{{varietyList}}" a:for-item="item" a:for-index="index" a:key="*this"><block><view class="weui-select" data-index="{{index}}" data-event-opts="{{[['tap',[['selectVariety',['$event']]]]]}}" onTap="__e"><view>{{item.petVarietyName}}</view></view></block></block></view></block><block a:else><view class="fss"><view class="left-box"><block a:for="{{categoryList}}" a:for-item="item" a:for-index="index" a:key="*this"><block><view class="p10-15 bgwh" data-id="{{item.petCategoryId}}" data-event-opts="{{[['tap',[['selectCategory',['$event']]]]]}}" onTap="__e"><view class="{{(petCategoryId==item.petCategoryId?'gor':'')}}">{{item.categoryName}}</view></view></block></block></view><view class="right-box"><block a:for="{{varietyList}}" a:for-item="item" a:for-index="index" a:key="*this"><block><view class="weui-select bgwh" data-index="{{index}}" data-event-opts="{{[['tap',[['selectVariety',['$event']]]]]}}" onTap="__e"><view>{{item.petVarietyName}}</view></view></block></block></view></view></block></view></view>