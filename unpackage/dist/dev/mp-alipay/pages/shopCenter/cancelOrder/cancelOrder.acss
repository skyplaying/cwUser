.submitBtn {
  color: #0e5e56;
  font-size: 32rpx;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 0px;
  background-color: #a8d8b9;
  text-align: center;
  border: 2rpx solid rgba(255, 255, 255, 0);
  position: absolute;
  bottom: 0;
}
.content {
  padding: 50rpx 60rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.content .t1 {
  color: #101010;
  font-size: 56rpx;
}
.content .t2 {
  margin-left: 145rpx;
  background-color: #ffffff;
  color: #999999;
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
  border: 2rpx solid rgba(255, 255, 255, 0);
  display: inline-block;
  vertical-align: middle;
  position: relative;
  top: -15rpx;
}
.content .radioGroup {
  height: 430rpx;
}
.content .radioGroup > label {
  margin-top: 25rpx;
  width: 100%;
  height: 45rpx;
  display: block;
}
.content .radioGroup > label > text {
  margin-left: 15rpx;
  vertical-align: middle;
}
.content .resonBox {
  margin-top: 80rpx;
}
.content .resonBox .r1 {
  color: #101010;
  font-size: 28rpx;
}
.content .resonBox radio {
  vertical-align: middle;
}

