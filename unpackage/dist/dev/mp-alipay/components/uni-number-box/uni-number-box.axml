<view class="uni-numbox"><block a:if="{{isPlus}}"><view data-event-opts="{{[['tap',[['_calcValue',['subtract']]]]]}}" class="{{(('minus')+' '+((disableSubtract||disabled)?'uni-numbox--disabled':''))}}" onTap="__e"><text class="minText">-</text></view></block><block a:if="{{isPlus}}"><input class="value" type="number" disabled="{{disabled}}" data-event-opts="{{[['blur',[['_onBlur',['$event']]]]]}}" value="{{inputValue}}" onBlur="__e"/></block><view data-event-opts="{{[['tap',[['_calcValue',['add']]]]]}}" class="{{(('plus ')+' '+((disableAdd||disabled)?'uni-numbox--disabled':''))}}" onTap="__e"><text class="plusText">+</text></view></view>