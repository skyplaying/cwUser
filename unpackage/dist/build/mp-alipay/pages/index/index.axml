<view class="page"><block a:if="{{isok}}"><view class="conatiner"><view class="bannerBox"><swiper class="banner1" style="height:328rpx;" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" circular="{{true}}" interval="{{interval}}" duration="{{duration}}"><block a:for="{{bannerList1}}" a:for-item="item" a:for-index="index" a:key="index"><block><swiper-item><view data-event-opts="{{[['tap',[['getIndex',[index]]]]]}}" class="Img" onTap="__e"><image src="{{item.img}}" mode="widthFix"></image></view></swiper-item></block></block></swiper><swiper class="banner2" style="height:268rpx;" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" circular="{{true}}" interval="{{interval}}" duration="{{duration}}"><block a:for="{{bannerList1}}" a:for-item="item" a:for-index="index" a:key="index"><block><swiper-item class="sw2"><view data-event-opts="{{[['tap',[['getIndex',[index]]]]]}}" class="Img" onTap="__e"><image src="{{item.img}}" mode="widthFix"></image></view></swiper-item></block></block></swiper></view><view class="flowBox"><view><image src="/static/index/yuyue.png"></image><view>预约服务</view></view><view><image src="/static/index/shangmen.png"></image><view>上门迎接</view></view><view><image src="/static/index/qingjie.png"></image><view>清洁整理</view></view><view><image src="/static/index/yuyue.png"></image><view>正式告别</view></view><view><image src="/static/index/huiyi.png"></image><view>永恒回忆</view></view></view><button data-event-opts="{{[['tap',[['next',['$event']]]]]}}" class="reserve" onTap="__e">预约服务</button><view><swiper class="tipList" autoplay="{{autoplay}}" vertical="{{true}}" circular="{{true}}" interval="{{interval}}" duration="{{500}}"><block a:for="{{fontList}}" a:for-item="item" a:for-index="index" a:key="index"><block><swiper-item><image class="tipImg" src="../../static/index/jingao.png"></image><text>香港法规</text><text>{{item}}</text></swiper-item></block></block></swiper></view><view class="story"><image class="gushi" src="../../static/index/gushi.png">